{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "simulator": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "previewTest": {
        "developmentClient": true
      },
      "production": {}
    },
    "submit": {
      "production": {}
    }
  }